{
    "_version": "1.12.0",
    "sap.app": {
        "id": "flovogt.website",
        "type": "application",
        "title": "Florian Vogt",
        "applicationVersion": {
            "version": "1.0.0"
        }
    },
    "sap.ui5": {
        "contentDensities": {
            "cozy": true,
            "compact": true
        },
        "dependencies": {
            "minUI5Version": "1.120.1",
            "libs": {
                "sap.ui.core": {},
                "sap.m": {}
            }
        },
        "rootView": {
            "viewName": "flovogt.website.view.App",
            "type": "XML",
            "id": "app"
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "flovogt.website.i18n.i18n",
                    "supportedLocales": [
                        "en",
                        "de"
                    ],
                    "fallbackLocale": "en"
                }
            },
            "": {
                "type": "sap.ui.model.json.JSONModel",
                "uri": "model/content.json"
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/styles.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "viewPath": "flovogt.website.view",
                "controlId": "app",
                "controlAggregation": "pages",
                "bypassed": {
                    "target": [
                        "notFound"
                    ]
                }
            },
            "routes": [
                {
                    "pattern": "",
                    "name": "home",
                    "target": [
                        "home"
                    ]
                }
            ],
            "targets": {
                "home": {
                    "viewName": "Home",
                    "viewId": "home",
                    "viewLevel": 1,
                    "title": "{i18n>homeViewTitle}"
                },
                "notFound": {
                    "viewName": "NotFound",
                    "viewId": "notFound"
                }
            }
        }
    }
}