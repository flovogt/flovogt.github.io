{
    "_version": "2.0.0",
    "sap.app": {
        "id": "flovogt.website",
        "type": "application",
        "title": "Florian Vogt",
        "applicationVersion": {
            "version": "1.0.0"
        }
    },
    "sap.ui5": {
        "contentDensities": {
            "cozy": true,
            "compact": true
        },
        "dependencies": {
            "minUI5Version": ["1.136.0", "2.0.0"],
            "libs": {
                "sap.ui.core": {},
                "sap.m": {},
                "sap.f": {}
            }
        },
        "rootView": {
            "viewName": "flovogt.website.view.App",
            "type": "XML",
            "id": "app"
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "flovogt.website.i18n.i18n",
                    "supportedLocales": [
                       ""
                    ],
                    "fallbackLocale": ""
                }
            },
            "": {
                "type": "sap.ui.model.json.JSONModel",
                "uri": "model/data.json"
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/styles.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "type": "View",
                "viewType": "XML",
                "path": "flovogt.website.view",
                "controlId": "app",
                "controlAggregation": "pages",
                "bypassed": {
                    "target": [
                        "home"
                    ]
                }
            },
            "routes": [
                {
                    "pattern": "",
                    "name": "home",
                    "target": [
                        "home"
                    ]
                },
                {
                    "pattern": "about",
                    "name": "about",
                    "target": [
                        "about"
                    ]
                },
                {
                    "pattern": "projects",
                    "name": "projects",
                    "target": [
                        "projects"
                    ]
                }
            ],
            "targets": {
                "header": {
                    "name": "Header",
                    "id": "header",
                    "level": 1,
                    "title": "{i18n>homeViewTitle}"
                },
                "home": {
                    "name": "Home",
                    "id": "home",
                    "title": "{i18n>homeViewTitle}",
                    "level": 2,
                    "parent": "header",
                    "controlId": "topics",
                    "controlAggregation": "items",
                    "clearControlAggregation": true
                },
                "about": {
                    "name": "About",
                    "id": "about",
                    "title": "{i18n>aboutViewTitle}",
                    "level": 2,
                    "parent": "header",
                    "controlId": "topics",
                    "controlAggregation": "items",
                    "clearControlAggregation": true
                },
                "projects": {
                    "name": "Projects",
                    "id": "projects",
                    "title": "{i18n>projectsViewTitle}",
                    "level": 2,
                    "parent": "header",
                    "controlId": "topics",
                    "controlAggregation": "items",
                    "clearControlAggregation": true
                }
            }
        }
    }
}